# DC Dimming
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display-primary/msm_fb_ea_enable                                                         u:object_r:vendor_sysfs_graphics:s0

# Display count
genfscon sysfs /devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/disp_count                                                u:object_r:vendor_sysfs_displaycount:s0

# Fingerprint
genfscon sysfs /devices/platform/soc/soc:fingerprint_fpc/                                                                                  u:object_r:vendor_sysfs_fingerprint:s0

# Last kmsg
genfscon proc /last_kmsg                                                                                                                   u:object_r:proc_last_kmsg:s0

# LEDs
genfscon sysfs /devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-05/c440000.qcom,spmi:qcom,pm8150l@5:qcom,leds@d000/leds/white          u:object_r:vendor_sysfs_graphics:s0

# Subsystem restart
genfscon sysfs /devices/platform/soc/soc:qcom,mdm3/subsys9/restart_level                                                                   u:object_r:vendor_sysfs_ssr_toggle:s0

# Wakeup source stats
genfscon sysfs /devices/0305_01.01.00/wakeup                                                                                                       u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/goodix_ts.0/wakeup                                                                                                u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/18800000.qcom,icnss/wakeup                                                                                    u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/890000.i2c/i2c-0/0-0010/890000.i2c:qcom,smb1390@10:qcom,charge_pump/wakeup                                    u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/a84000.i2c/i2c-2/2-0028/wakeup                                                                                u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/c80000.i2c/i2c-3/3-0049/wakeup                                                                                u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/171c0000.slim/tavil-slim-pgd/wakeup                                                                           u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/1c08000.qcom,pcie/pci0001:00/0001:00:00.0/0001:01:00.0/0305_01.01.00_EFS/wakeup                               u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/vendor/vendor:extcon_usb1/wakeup                                                                                  u:object_r:sysfs_wakeup:s0
